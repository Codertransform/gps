<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yibo.gps.dao.TransDao">

    <insert id="insert">
        insert into transform_gps_data(
            id,
            origin_id,
            latitude,
            longitude
        )
        values (
            #{id},
            #{originId},
            #{latitude},
            #{longitude}
        )
    </insert>
</mapper>