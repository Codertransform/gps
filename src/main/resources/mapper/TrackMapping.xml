<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yibo.gps.dao.TrackMapper">

    <insert id="insert">
        insert into track(
            id,
            track_id,
            car_id,
            start_time,
            end_time,
            miles
        )
        values (
            #{id},
            #{trackId},
            #{carId},
            #{startTime},
            #{endTime},
            #{miles}
        )
    </insert>
</mapper>